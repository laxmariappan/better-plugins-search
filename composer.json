{
  "name": "LaxMariappan/better-plugins-search",
  "description": "A plugin to add search fields",
  "type": "wordpress-plugin",
  "require-dev": {
    "phpcompatibility/phpcompatibility-wp": "dev-master",
    "dealerdirect/phpcodesniffer-composer-installer": "dev-master",
    "wp-coding-standards/wpcs": "dev-master",
    "squizlabs/php_codesniffer": "^3.0@dev"
  },
  "scripts": {
    "format": "./vendor/bin/phpcbf --standard=.phpcs.xml --report=summary,source",
    "lint": "./vendor/bin/phpcs --standard=.phpcs.xml --report=summary,source",
    "pot": "./vendor/wp-cli/wp-cli/bin/wp i18n make-pot . lang/ajax-load-more.pot --exclude=node_modules,vendor,core/dist --allow-root"
  },
  "license": "GPL-3.0+",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Lax Mariappan",
      "email": "lax@laxmariappan.com"
    }
  ]
}
